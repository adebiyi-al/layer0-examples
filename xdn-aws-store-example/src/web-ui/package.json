{
  "name": "retail-demo-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "xdn:start": "concurrently \"npx webpack --watch --config xdn/webpack.xdn.config.js\" \"npx xdn run\"",
    "xdn:start:cache": "concurrently \"npx webpack --watch --config xdn/webpack.xdn.config.js\" \"npx xdn run --cache\"",
    "xdn:start:prod": "npx xdn run --production",
    "xdn:build": "npx webpack --progress --config xdn/webpack.xdn.config.js && npx xdn build",
    "xdn:deploy": "npx xdn deploy --team=moovweb-demos --skip-build --site=aws-rds",
    "xdn:update": "npm i -D @xdn/cli@latest && npm i @xdn/core@latest @xdn/prefetch@latest @xdn/devtools@latest"
  },
  "dependencies": {
    "@optimizely/optimizely-sdk": "4.0.0",
    "@xdn/core": "^2.29.0",
    "@xdn/devtools": "^2.29.0",
    "@xdn/prefetch": "^2.29.0",
    "amplitude-js": "^5.8.0",
    "aws-amplify": "^3.0.23",
    "aws-amplify-vue": "^2.1.2",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "sweetalert": "^2.1.2",
    "uuid": "^7.0.3",
    "vue": "^2.6.11",
    "vue-router": "^3.4.2",
    "vuex": "^3.5.1",
    "vuex-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.2",
    "@vue/cli-plugin-eslint": "^4.5.2",
    "@vue/cli-service": "^4.5.2",
    "@xdn/cli": "^2.29.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "is-buffer": "^2.0.4",
    "lodash": "^4.17.19",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^4.2.0"
  },
  "resolutions": {
    "acorn": "^6.4.1",
    "elliptic": "^6.5.3",
    "kind-of": "^6.0.3",
    "minimist": "^1.2.5",
    "selfsigned": "^1.10.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
